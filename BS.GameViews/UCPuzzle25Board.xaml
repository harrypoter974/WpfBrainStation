<UserControl x:Class="BS.GameViews.UCPuzzle25Board"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Visibility="{Binding IsVisibility}">
    <Grid>
        <!--<Grid.Background> -->
        <!--<ImageBrush ImageSource="C:\Brain_Station\WpfBrainStationBrachoot\WpfBrainStation\Resources\Game\UCPuzzle25\Gcard.png"/>-->
        <!-- <ImageBrush ImageSource="{Binding BackgroundCard}"/>
        </Grid.Background>-->
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="18*"/>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="53*"/>
            <ColumnDefinition Width="53*"/>
            <ColumnDefinition Width="53*"/>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="18*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="18*"/>
            <RowDefinition Height="52*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="52*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="18*"/>
        </Grid.RowDefinitions>
        <Image Source="{Binding BackgroundCard}" Grid.ColumnSpan="7" Grid.RowSpan="7" Margin="6,6,6,6"/>
        <Ellipse  Grid.Row="1" Fill="{Binding Bull00}" Grid.Column="1"  />
        <Ellipse Grid.Column="2"  Grid.Row="1" Fill="{Binding Bull01}" />
        <Ellipse Grid.Column="3"  Grid.Row="1" Fill="{Binding Bull02}" />
        <Ellipse Grid.Column="4"  Grid.Row="1" Fill="{Binding Bull03}"/>
        <Ellipse Grid.Column="5"  Grid.Row="1" Fill="{Binding Bull04}" />
        <Ellipse  Grid.Row="2" Fill="{Binding Bull10}" Grid.Column="1" />
        <Ellipse Grid.Column="2"  Grid.Row="2" Fill="{Binding Bull11}" />
        <Ellipse Grid.Column="3"  Grid.Row="2" Fill="{Binding Bull12}" />
        <Ellipse Grid.Column="4"  Grid.Row="2" Fill="{Binding Bull13}" />
        <Ellipse Grid.Column="5"  Grid.Row="2" Fill="{Binding Bull14}" />
        <Ellipse  Grid.Row="3" Fill="{Binding Bull20}" Grid.Column="1"/>
        <Ellipse Grid.Column="2"  Grid.Row="3" Fill="{Binding Bull21}"  />
        <Ellipse Grid.Column="3"  Grid.Row="3" Fill="{Binding Bull22}"  />
        <Ellipse Grid.Column="4"  Grid.Row="3" Fill="{Binding Bull23}"  />
        <Ellipse Grid.Column="5"  Grid.Row="3" Fill="{Binding Bull24}"  />
        <Ellipse  Grid.Row="4" Fill="{Binding Bull30}" Grid.Column="1"/>
        <Ellipse Grid.Column="2"  Grid.Row="4" Fill="{Binding Bull31}"/>
        <Ellipse Grid.Column="3"  Grid.Row="4" Fill="{Binding Bull32}"/>
        <Ellipse Grid.Column="4"  Grid.Row="4" Fill="{Binding Bull33}"/>
        <Ellipse Grid.Column="5"  Grid.Row="4" Fill="{Binding Bull34}"/>
        <Ellipse  Grid.Row="5" Fill="{Binding Bull40}" Grid.Column="1" />
        <Ellipse Grid.Column="2"  Grid.Row="5" Fill="{Binding Bull41}"  />
        <Ellipse Grid.Column="3"  Grid.Row="5" Fill="{Binding Bull42}"  />
        <Ellipse Grid.Column="4"  Grid.Row="5" Fill="{Binding Bull43}"  />
        <Ellipse Grid.Column="5"  Grid.Row="5" Fill="{Binding Bull44}"  />

    </Grid>
</UserControl>
