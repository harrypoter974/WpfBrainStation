<UserControl x:Class="BS.MenuViews.UCMenuPrint"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BS.MenuViews"
             mc:Ignorable="d" 
                   d:DesignHeight="900" d:DesignWidth="1000"  >
    <UserControl.Background>
        <ImageBrush ImageSource="MenuImage/UCMenuPrint.jpg"/>
    </UserControl.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="34*"/>
            <ColumnDefinition Width="251*"/>
            <ColumnDefinition Width="334*"/>
            <ColumnDefinition Width="281*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="54*"/>
            <RowDefinition Height="333*"/>
            <RowDefinition Height="89*"/>
            <RowDefinition Height="38*"/>
            <RowDefinition Height="88*"/>
            <RowDefinition Height="38*"/>
            <RowDefinition Height="88*"/>
            <RowDefinition Height="39*"/>
            <RowDefinition Height="88*"/>
            <RowDefinition Height="145*"/>
        </Grid.RowDefinitions>
        <Label x:Name="labelHome"  >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="MenuLernVM"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="LPrint" Grid.Column="2"  Grid.Row="2"  >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter=""/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="label2" Grid.Column="2"  Grid.Row="4"  >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter=""/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="label3" Grid.Column="2"  Grid.Row="6"  >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter=""/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="label4" Grid.Column="2"  Grid.Row="8"  >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter=""/>
            </Label.InputBindings>
        </Label>
    </Grid>
</UserControl>
