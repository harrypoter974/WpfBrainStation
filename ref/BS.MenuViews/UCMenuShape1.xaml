<UserControl x:Class="BS.MenuViews.UCMenuShape1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BS.MenuViews"
             mc:Ignorable="d" 
                 d:DesignHeight="900" d:DesignWidth="1000"  >
    <UserControl.Background>
        <ImageBrush ImageSource="MenuImage/UCMenuShape.jpg"/>
    </UserControl.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="47*" />
            <RowDefinition Height="36*" />
            <RowDefinition Height="27*"/>
            <RowDefinition Height="28*"/>
            <RowDefinition Height="62*"/>
            <RowDefinition Height="57*"/>
            <RowDefinition Height="77*" />
            <RowDefinition Height="9*" />
            <RowDefinition Height="78*" />
            <RowDefinition Height="8*" />
            <RowDefinition Height="78*" />
            <RowDefinition Height="9*" />
            <RowDefinition Height="78*" />
            <RowDefinition Height="9*" />
            <RowDefinition Height="81*" />
            <RowDefinition Height="9*"/>
            <RowDefinition Height="75*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="14*" />
            <RowDefinition Height="54*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="44*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="39*" />
            <ColumnDefinition Width="221*" />
            <ColumnDefinition Width="17*" />
            <ColumnDefinition Width="35*"/>
            <ColumnDefinition Width="21*"/>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="389*"/>
            <ColumnDefinition Width="261*" />
        </Grid.ColumnDefinitions>
        <Label Grid.Column="2" Grid.Row="6" Name="labelLine"  Grid.ColumnSpan="5">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="LineOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label Grid.Column="2" Grid.Row="8"  Name="labelAngle"   Grid.ColumnSpan="5" >
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="AngleOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label Grid.Column="2" Grid.Row="10" Name="labelTringel" Grid.ColumnSpan="5">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="TringelOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label Grid.Column="2" Grid.Row="12" Name="labelRect"    Grid.ColumnSpan="5">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="RectOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label Grid.Column="2" Grid.Row="14" Name="labelBull"    Grid.ColumnSpan="5">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="CirclesOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="labelShape"  Grid.Column="2" Grid.Row="16"        Grid.ColumnSpan="5">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="ShapeOpenVM"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="labelRecognition" Grid.Column="2" Grid.Row="18" Grid.ColumnSpan="5" Grid.RowSpan="3">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="ShapeGameVM"/>
            </Label.InputBindings>
        </Label>
        <Label  Name="labelHome"   >
     <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}" CommandParameter="MenuLernVM"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="LHelp"  Grid.Column="3"  Grid.Row="4">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="LHelpGame"  Grid.Column="3" Grid.Row="19">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}"/>
            </Label.InputBindings>
        </Label>
        <Label x:Name="LCloseHelp" Grid.Column="5" Grid.Row="2">
            <Label.InputBindings>
                <MouseBinding MouseAction="LeftClick"  Command="{Binding GoTo1}"/>
            </Label.InputBindings>
        </Label>
    </Grid>
</UserControl>
