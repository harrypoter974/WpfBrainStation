<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BS.MenuViews"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:Items="clr-namespace:BS.Items;assembly=BS.Items" x:Class="BS.MenuViews.UCMenuVocabulary"
             mc:Ignorable="d" 
               d:DesignHeight="900" d:DesignWidth="1000"   >
    <UserControl.Background>
        <ImageBrush ImageSource="MenuImage/UCMenuVocabulary.jpg"/>
    </UserControl.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="8*"/>
            <ColumnDefinition Width="26*"/>
            <ColumnDefinition Width="6*"/>
            <ColumnDefinition Width="58*"/>
            <ColumnDefinition Width="56*"/>
            <ColumnDefinition Width="21*"/>
            <ColumnDefinition Width="56*"/>
            <ColumnDefinition Width="57*"/>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="57*"/>
            <ColumnDefinition Width="59*"/>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="56*"/>
            <ColumnDefinition Width="58*"/>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="57*"/>
            <ColumnDefinition Width="56*"/>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="45*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="6*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="57*"/>
            <ColumnDefinition Width="43*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="11*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="247*"/>
            <RowDefinition Height="246*"/>
            <RowDefinition Height="60*"/>
            <RowDefinition Height="234*"/>
            <RowDefinition Height="64*"/>
        </Grid.RowDefinitions>

        <Label  Grid.ColumnSpan="3"  Grid.RowSpan="4"  >
            <Label.Background>
                <ImageBrush ImageSource="{Binding HomeBut}"  Stretch="None"/>
            </Label.Background>
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GOHome}" CommandParameter="MenuHeGeneralRulesWeightedVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>

        <Label  Grid.Column="3"  Grid.Row="5" Grid.ColumnSpan="2" Background="{Binding BackgroundBut0}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuKitchenwareVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label  Grid.Column="6"  Grid.Row="5" Grid.ColumnSpan="2" Background="{Binding BackgroundBut1}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuFoodVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="9"   Grid.Row="5" Grid.ColumnSpan="2" Background="{Binding BackgroundBut2}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuFruitVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="12"  Grid.Row="5" Grid.ColumnSpan="2" Background="{Binding BackgroundBut3}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuVegetablesVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="15"   Grid.Row="5" Grid.ColumnSpan="2" Background="{Binding BackgroundBut4}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuBodyVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="18"  Grid.ColumnSpan="4" Grid.Row="5" Background="{Binding BackgroundBut5}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="AnimalsMenuVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="23"  Grid.Row="5" Grid.ColumnSpan="3" Background="{Binding BackgroundBut6}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuClothingVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="2"   Grid.Row="7" Grid.ColumnSpan="4" Background="{Binding BackgroundBut7}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuVehiclesVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="6"  Grid.Row="7" Grid.ColumnSpan="2" Background="{Binding BackgroundBut8}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuProfessionsVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="9"   Grid.Row="7" Grid.ColumnSpan="2" Background="{Binding BackgroundBut9}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuMusicalLnstrumentsVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="11"   Grid.Row="7"  Grid.ColumnSpan="3" Background="{Binding BackgroundBut10}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuFamilyVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>

        <Label Grid.Column="15"   Grid.Row="7" Grid.ColumnSpan="2" Background="{Binding BackgroundBut11}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuToolsVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="18"  Grid.Row="7" Grid.ColumnSpan="4" Background="{Binding BackgroundBut12}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="MenuShapesVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label Grid.Column="23"   Grid.Row="7" Grid.ColumnSpan="3" Background="{Binding BackgroundBut13}" >
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="TouchDown">
                    <i:InvokeCommandAction Command="{Binding GoToPage}" CommandParameter="ColorsMenuVM" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Label>
        <Label  Grid.Column="19"  Grid.Row="2" Grid.ColumnSpan="2"/>
        <Label  Grid.Column="21"  Grid.Row="2"  Grid.ColumnSpan="3"/>
        <Label  Grid.Column="24"  Grid.Row="2" />
        <Items:NUCBlueUCSpeaker  Grid.ColumnSpan="5" Grid.RowSpan="5"
  VerticalAlignment="Top"  Margin="{Binding SpeakerMargin}" HorizontalAlignment="Left"/>
    </Grid>
</UserControl>
